<%Server.Execute("include/user_init.html")%><?xml version="1.0" encoding="windows-1251"?>
<competences name="<%=tools.get_web_str("ass_competences")%>">
<%
	for (_competence in competences)
	{
%>
	<competence name="<%=HtmlEncode(tools_web.get_cur_lng_name(_competence.name.Value, curLng.short_id))%>" id="<%=_competence.id%>" top="1">
<%
		for (_indicator in indicators)
		{
			if (_indicator.competence_id == _competence.id)
			{
%>		
			<competence id="<%=_indicator.id%>" name="<%=HtmlEncode(tools_web.get_cur_lng_name(_indicator.name.Value, curLng.short_id))%>" type="<%=_indicator.type%>" parent_id="<%=_competence.id%>"/>
<%
			}
		}
%>		
	</competence>
<%
	}
%>
</competences>